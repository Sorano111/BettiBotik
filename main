@slash.slash(name = 'clear', description = 'очистить чат', options = [{"name": "amount", "description": "сума сообщений","type": 10, "required": False}], guild_ids = None)
@client.command(pass_context = True)
@commands.has_permissions(administrator= True)
async def clear(ctx, amount=1000):
    await ctx.channel.purge(limit = amount)


@client.command(pass_context = True)
@commands.has_permissions(administrator = True)
async def kick(ctx, member: discord.Member, *, reason=None):
    await member.kick(reason = reason)
    embed = discord.Embed(title = f'Kick user {member}', color = 0xff80ff)
    kicked = await ctx.send(embed = embed)
    await kicked.add_reaction('<a:emoji_36:937995398444904448>')


@client.command()
@commands.has_permissions(administrator = True)
async def mute(ctx, member: discord.Member,*, time: int = None):
    mute_role = discord.utils.get(member.guild.roles, name = 'mute')
    await member.add_roles(mute_role)
    embed = discord.Embed(title = f'У {member} ограничение чата за нарушение прав!', color = 0xff80ff)
    muted = await ctx.send(embed = embed)
    await muted.add_reaction('<a:emoji_36:937995398444904448>')
    await asyncio.sleep(time * 60)
    await member.remove_roles(mute_role)



