@slash.slash(name = 'user', description = 'информация о пользователе', options = [{"name": "member", "description": "пользователь","type": 6, "required": False}], guild_ids = None)
@client.command(name = 'user')
async def user(ctx, member: discord.Member = None):
    if member == None:
        member = ctx.author
    roles = [role for role in member.roles]
    embed = discord.Embed(title = f'***Информация о пользователе {member.name}***', color = 0xff80ff)
    embed.set_thumbnail(url = member.avatar_url)
    embed.add_field(name = 'ID', value = member.id, inline = True)
    embed.add_field(name = 'Никнейм', value = member.display_name, inline = True)
    embed.add_field(name = 'Аккаунт создан:', value = member.created_at.strftime('%d.%m.%Y'), inline = True)
    embed.add_field(name = 'Присоединился к серверу:', value = member.joined_at.strftime('%d.%m.%Y'), inline = True)
    embed.add_field(name = 'Роли:', value = " ".join(role.mention for role in roles), inline = True)
    embed.set_footer(text = ctx.author.name, icon_url = ctx.author.avatar_url)
    await ctx.send(embed = embed)


@slash.slash(name = 'create', description = 'создать розыгрыш', options = [{"name": "time", "description": "время","type": 3, "required": True},{"name": "prize", "description": "приз","type": 3, "required":True}], guild_ids = None)
@client.command(name="create")
@commands.has_permissions(administrator = True)
async def create(ctx, time=None, *, prize=None):
    if time == None:
        return await ctx.send(embed = discord.Embed(title = f'🕟 Укажите время!', color = 0xff80ff))
    elif prize == None:
        return await ctx.send(embed = discord.Embed(title = f' 🎁Укажите приз!', color = 0xff80ff))
    embed = discord.Embed(title = '🎉Розыгрыш', description = f'🎁Приз: **{prize}**!', color = 0xff80ff)
    time_convert = {"d": 86400, "h": 3600, "m": 60, "s": 1}
    gawtime = int(time[0]) * time_convert[time[-1]]
    embed.set_footer(text = f'🕟Розыгрыш закончиться через {time}')
    gaw_msg = await ctx.send(embed = embed)
    await gaw_msg.add_reaction("🎉")
    await asyncio.sleep(gawtime)
    new_gaw_msg = await ctx.channel.fetch_message(gaw_msg.id)
    users = await new_gaw_msg.reactions[0].users().flatten()
    users.pop(users.index(client.user))
    winner = random.choice(users)
    await ctx.send(embed = discord.Embed(title = '🥳Выиграл:', description = f'{winner}', color = 0xff80ff))


@slash.slash(description = 'информация о сервере')
async def server(ctx):
    icon = str(ctx.guild.icon_url)
    guild = ctx.guild
    role_count = len(ctx.guild.roles)
    online=0
    offline=0
    idle=0
    text=0
    voice=0
    dnd=0
    for member in ctx.guild.members:
        if str(member.status)=="online":
            online+=1
        if str(member.status)=="idle":
            idle+=1
        if str(member.status)=="offline":
            offline+=1
        if str(member.status)=="dnd":
            dnd+=1
    for channel in ctx.guild.channels:
        if str(channel.type)=="text":
            text+=1
        if str (channel.type)=="voice":
            voice+=1
    list_of_bots = [bot.mention for bot in ctx.guild.members if bot.bot]
    serverinfoEmbed = discord.Embed( color = 0xff80ff)
    serverinfoEmbed.add_field(name = '🪧Название:', value = f'{guild.name}', inline = False)
    serverinfoEmbed.add_field(name = '🧾ID:', value = str(guild.id), inline = False)
    serverinfoEmbed.add_field(name = '🌏Количество учасников:', value = guild.member_count, inline = False)
    serverinfoEmbed.add_field(name = '📊 Уровень:', value = str(guild.verification_level), inline = False)
    serverinfoEmbed.add_field(name = '🗓️ Сервер создан:', value = guild.created_at.strftime('%d.%m.%Y'), inline = True)
    serverinfoEmbed.add_field(name = '🗂️Количество ролей:', value = str(role_count), inline = False)
    serverinfoEmbed.add_field(name = 'Боты:', value = ', '.join(list_of_bots), inline = False)
    serverinfoEmbed.add_field(name = 'Статус учасников:', value = f'<:online:939130426159685663>**Онлайн:** {online}\n<:idle:939130402038243348>**Не активен:** {idle}\n<:dnd:940204819195510795>**Не беспокоить**:{dnd}\n <:offline:939130444417482842>**Офлайн:** {offline} ', inline = False)
    serverinfoEmbed.add_field(name = 'Каналы:', value = f'✉**Текстовых:** {text}\n🎙️**Голосовых:** {voice}', inline = False)
    serverinfoEmbed.set_thumbnail(url = icon)
    await ctx.send(embed = serverinfoEmbed)


